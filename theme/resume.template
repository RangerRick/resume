<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>{{basics.name}}'s Resume</title>
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
		<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
		<link href='http://fonts.googleapis.com/css?family=Maven Pro|PT Sans' rel='stylesheet' type='text/css'>
		<style type="text/css">
			{{{css}}}
		</style>
	</head>
	<body id="top">
		<div id="cv">
			<div class="mainDetails">
				<div id="headshot">
					<img src="{{photo}}" alt="avatar" />
				</div>
				<div id="name">
					<h1>{{basics.name}}</h1>
					<h2>{{basics.label}}</h2>
				</div>
				<div id="contactDetails">
					{{#basics.location}}
                        <div class="contact-item location">
                          <div class="icon pull-left text-center"><span class="fa fa-map-marker fa-fw"></span></div>
						  <div class="title {{^basics.location.address}}only {{/basics.location.address}}pull-right">
							{{#basics.location.address}}
							<div class="address">{{basics.location.address}}</div>
							{{/basics.location.address}}
							<div class="postal">
								{{basics.location.city}}
								{{#basics.location.postalCode}}{{basics.location.postalCode}}{{/basics.location.postalCode}}
							</div>
						  </div>
                        </div>
                    {{/basics.location}}
                    {{#basics.phone}}
                        <div class="contact-item">
                          <div class="icon pull-left text-center"><a href="tel:{{basics.phone}}"><span class="fa fa-phone fa-fw"></span></a></div>
                          <div class="title only pull-right"><a href="tel:{{basics.phone}}">{{basics.phone}}</a></div>
                        </div>
                    {{/basics.phone}}
                    {{#basics.email}}
                        <div class="contact-item">
                          <div class="icon pull-left text-center"><span class="fa fa-envelope fa-fw"></span></div>
                          <div class="title only pull-right"><a href="mailto:{{basics.email}}" target="_blank">{{basics.email}}</a></div>
                        </div>
                    {{/basics.email}}
                    {{#basics.website}}
                        <div class="contact-item">
                          <div class="icon pull-left text-center"><span class="fa fa-globe fa-fw"></span></div>
                          <div class="title only pull-right"><a href="{{basics.website}}" target="_blank">{{basics.website}}</a></div>
                        </div>
                    {{/basics.website}}
                    {{#basics.profiles}}
                        <div class="contact-item">
                          <div class="icon pull-left text-center"><span class="{{iconClass}} fa-fw"></span></div>
                          <div class="description pull-right"><a href="{{url}}" target="_blank">{{#username}}{{username}}{{/username}}{{^username}}{{url}}{{/username}}</a></div>
                        </div>
                    {{/basics.profiles}}
				</div>
				<div class="clear"></div>
			</div>
			<div id="mainArea">
				{{#basics.summary}}
				<section>
					<article>
						<div class="sectionTitle">
							<h1>About</h1>
						</div>
						<div class="sectionContent">
							<p>{{{basics.summary}}}</p>
						</div>
					</article>
					<div class="clear"></div>
				</section>
				{{/basics.summary}}
				{{#workBool}}
				<section>
					<div class="sectionTitle">
						<h1>Work</h1>
					</div>
					<div class="sectionContent">
						{{#work}}
						<article>
							<h1>
							{{#url}}
                            <a href="{{url}}">{{name}}</a>
							{{/url}}
							{{^url}}
                            {{name}}
							{{/url}}
							</h1>
							<h2>{{position}}</h2>
							{{#startDate}}
                            <p class="subDetails">{{startDateMonth}}{{startDateYear}} – {{endDateMonth}}{{endDateYear}} &bull; {{location}}</p>
							{{/startDate}}
                            {{#boolHighlights}}
                                <ul class="info">
                                    {{#highlights}}
                                    <li>{{{.}}}</li>
                                    {{/highlights}}
                                </ul>
                            {{/boolHighlights}}
						</article>
						{{/work}}
					</div>
					<div class="clear"></div>
				</section>
				{{/workBool}}
				{{#allProjectsBool}}
				<section class="projects">
					<div class="sectionTitle">
						<h1>Projects</h1>
					</div>
					<div class="sectionContent">
						{{#volunteer}}
						<article>
							<h1>
							{{#url}}
                            <a href="{{url}}">{{organization}}</a>
							{{/url}}
							{{^url}}
                            {{organization}}
							{{/url}}
							</h1>
							{{#position}}
							<h2>{{position}}</h2>
							{{/position}}
							{{#startDate}}
                            <p class="subDetails">{{startDateMonth}}{{startDateYear}} – {{endDateMonth}}{{endDateYear}}</p>
							{{/startDate}}
							{{#boolHighlights}}
							<ul class="info">
							{{#highlights}}
							<li>{{{.}}}</li>
							{{/highlights}}
							</ul>
							{{/boolHighlights}}
						</article>
						{{/volunteer}}
						{{#projects}}
						<article>
							{{#url}}
                            <h1><a href="{{url}}">{{name}}</a></h1>
							{{/url}}
							{{^url}}
                            <h2>{{name}}</h2>
							{{/url}}
							{{#startDate}}
                            <p class="subDetails">{{startDateMonth}}{{startDateYear}} – {{endDateMonth}}{{endDateYear}}</p>
							{{/startDate}}
							<p class="description">{{description}}</p>
                            {{#boolHighlights}}
                                <ul class="info">
                                    {{#highlights}}
                                    <li>{{{.}}}</li>
                                    {{/highlights}}
                                </ul>
                            {{/boolHighlights}}
						</article>
						{{/projects}}
					</div>
					<div class="clear"></div>
				</section>
				{{/allProjectsBool}}
				{{#skillsBool}}
				<section class="skills">
					<div class="sectionTitle">
                        <h1>Skills</h1>
                    </div>
                    <div class="sectionContent">
                    {{#skills}}
					<article>
						<div class="info">
							<div class="skill-header">
                            <h2>
							{{name}}
							</h2>
							<!--
							{{#level}}
							<div class="skill-level {{level}}">{{level}}</div>
							{{/level}}
							-->
							</div>
							{{#keywordsBool}}
							<ul class="skills-keywords subDetails">
								{{#keywords}}
								<li class="skill badge">{{{.}}}</li>
								{{/keywords}}
							</ul>
							{{/keywordsBool}}
						</div>
					</article>
					{{/skills}}
					</div>
					<div class="clear"></div>
				</section>
				{{/skillsBool}}
			</div>
		</div>
		<script type="text/javascript">
			var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
			document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
		</script>
		<script type="text/javascript">
			var pageTracker = _gat._getTracker("UA-3753241-1");
			pageTracker._initData();
			pageTracker._trackPageview();
		</script>
	</body>
</html>
